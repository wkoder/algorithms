import java.io.*;
import java.util.*;
import java.math.*;
import java.text.*;

public class I {
	
	static final int INF = 1 << 28, MAX = 1000;
	static final double EPS = 1E-9;
	
	static void solve() {
		Scanner scan = new Scanner(System.in);
		int T = scan.nextInt();
		for (int t = 1; t <= T; t++) {
			int N = scan.nextInt();
			int x1 = scan.nextInt();
			int y1 = scan.nextInt();
			for (int i = 1; i < N/2; i++) {
				scan.nextInt();
				scan.nextInt();
			}
			int x2 = scan.nextInt();
			int y2 = scan.nextInt();
			for (int i = 1; i < N/2; i++) {
				scan.nextInt();
				scan.nextInt();
			}
			
		}
	}
	
	public static void main(String[] args) throws IOException {
		solve();
	}
}
