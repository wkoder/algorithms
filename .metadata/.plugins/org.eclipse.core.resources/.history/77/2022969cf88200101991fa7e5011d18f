import java.util.*;

public class Domino {
	
	private static String toChar(int t) {
		return (char)('a' + t) + "";
	}

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int N = scan.nextInt();
		if (N % 2 == 1) {
			System.out.println(-1);
			return;
		}
		
		String[] ans = {"", "", "", ""};
		int ch = 0;
		for (int i = 0; i < N; i++) {
			if (i % 2 == 0) {
				ans[0] += toChar(ch) + toChar(ch);
				ch = (ch + 1) % 26;
				
				ans[3] += toChar(ch) + toChar(ch);
				ch = (ch + 1) % 26;
			}
			
			if (i % 3 == 0 || N%3==1) {
				ans[1] += toChar(ch);
				ans[2] += toChar(ch);
				ch = (ch + 1) % 26;
			} else if (i % 3 == 1) {
				ans[1] += toChar(ch) + toChar(ch);
				ch = (ch + 1) % 26;
				
				ans[2] += toChar(ch) + toChar(ch);
				ch = (ch + 1) % 26;
			}
		}
		
		for (String a : ans)
			System.out.println(a);
	}
}
