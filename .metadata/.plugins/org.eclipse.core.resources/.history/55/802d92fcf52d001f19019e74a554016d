import java.util.Arrays;


public class NameInput {
	public int countUpDownKeyPresses(String[] predictionSequence, String[] name) {
		String seq = "";
		for (String s : predictionSequence)
			seq += s;
		String n = "";
		for (String s : name)
			n += s;
		int[] pos = new int[250];
		Arrays.fill(pos, -1);
		for (int i = 0; i < seq.length(); i++)
			pos[seq.charAt(i)] = i;
		
		int presses = 0;
		
		return presses;
	}
}
