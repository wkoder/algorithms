import java.util.ArrayList;
import java.util.Arrays;
import java.util.List;


public class ColoringRectangle {
	public int chooseDisks(int w, int h, int[] red, int[] blue) {
		Arrays.sort(red);
		Arrays.sort(blue);
		int[][] d = {red, blue};
		int min = 1000;
		for (int c = 0; c < 2; c++) {
			double x = 0;
			int color = c;
			int[] p = {d[c].length, d[c^1].length};
			while (true) {
				int pos = --p[color];
				if (pos < 0 || d[color][pos] < h)
					break;
				
				double hh = 2 * Math.sqrt(d[color][pos]/2.0*d[color][pos]/2.0 - h/2.0*h/2.0);
				x += hh;
				
				color ^= 1;
			}
			
		}
		return min < 1000? min : -1;
	}
}
