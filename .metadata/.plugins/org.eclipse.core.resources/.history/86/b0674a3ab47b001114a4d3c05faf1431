public class EllysFractions {
	long[][] choose;
	
	public long getCount(int N) {
		nChooseKDP(N, N);
		
		long count = 0;
		int primes = 0;
		for (int i = 2; i <= N; i++) {
			boolean prime = true;
			for (int j = 2; j*j <= i; j++)
				if (i % j == 0) {
					prime = false;
					break;
				}
			
			if (prime)
				primes++;
			
		}
		return count;
	}
	
	void nChooseKDP(int n, int k) {
		choose = new long[n + 1][k + 1];
		int i, j;
		for (i = 0; i <= n; i++)
			choose[i][0] = choose[i][i] = 1;
		for (i = 1; i <= n; i++)
			for (j = 1; j < i; j++)
				choose[i][j] = choose[i - 1][j - 1] + choose[i - 1][j];
	}
}
