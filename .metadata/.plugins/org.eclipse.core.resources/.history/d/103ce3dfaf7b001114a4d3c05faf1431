public class EllysJuice {
	
	double[] drank;
	
	String winner(String[] players, double[] drank) {
		
	}
	
	public String[] getWinners(String[] players) {
		drank = new double[N];
		int N = players.length;
		for (int i = 0; i < N; i++) {
			if (i < 2)
				drank[i] = 0.5;
			else
				drank[i] = drank[i - 2] / 2;
		}
		
		
	}
}
